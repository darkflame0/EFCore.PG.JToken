<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>Darkflame.EntityFrameworkCore.PostgreSQL.JToken</AssemblyName>
    <RootNamespace>Darkflame.EntityFrameworkCore.PostgreSQL.JToken</RootNamespace>
    <Authors>Darkflame</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/darkflame0/EFCore.PG.JToken</RepositoryUrl>
    <!--<TreatWarningsAsErrors>true</TreatWarningsAsErrors>-->
    <Nullable>enable</Nullable>
	<VersionPrefix>5.0.5</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>1701;1702;EF1001;</NoWarn>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="5.0.5" />
  </ItemGroup>

</Project>
